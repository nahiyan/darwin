; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Reference",
"class": "Configuration",
"language": "GDScript",
"path": "res://scripts/Configuration.gd"
} ]
_global_script_class_icons={
"Configuration": ""
}

[application]

config/name="Pong"
run/main_scene="res://scenes/ParallelEvolution.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://scripts/Global.gd"
Neat="*res://scripts/Neat.gdns"
PersistentModels="*res://scripts/PersistentModels.gdns"

[debug]

gdscript/warnings/unsafe_call_argument=true

[display]

window/size/resizable=false

[layer_names]

2d_physics/layer_1="paddles"
2d_physics/layer_2="ball"
2d_physics/layer_3="boundaries"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
