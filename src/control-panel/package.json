{
  "name": "darwin-control-panel",
  "version": "1.0.0",
  "description": "Control Panel of Darwin",
  "main": "main.js",
  "scripts": {
    "app:start": "electron-forge start",
    "app:package": "electron-forge package",
    "app:make": "electron-forge make",
    "build:watch": "npx webpack --mode development --watch"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "darwin",
    "control",
    "panel"
  ],
  "author": "Nahiyan Alamgir<nahiyan.alamgir0@gmail.com>, Momena Akhter Shukhi <makhter.shukhi@gmail.com>",
  "license": "CC0-1.0",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-deb": "^6.0.0-beta.54",
    "@electron-forge/maker-rpm": "^6.0.0-beta.54",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "bootstrap": "^5.0.0-beta3",
    "css-loader": "^5.2.0",
    "electron": "^12.0.2",
    "electron-reloader": "^1.2.0",
    "elm-webpack-loader": "^8.0.0",
    "postcss": "^8.2.9",
    "postcss-loader": "^5.2.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.30.0",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "toml": "^3.0.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_quick_start"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
